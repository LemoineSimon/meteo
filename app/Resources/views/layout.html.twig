<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">

        <title>{% block title %}[titre à définir]{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bootstrap3/css/bootstrap.min.css') }}" type="text/css" />
        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
        <![endif]-->
            {% block addcss %}{% endblock %}
        {% endblock %}
    </head>

    <body>

    {% block body %}{% endblock %}

    {% block javascripts %}
        <script src="//code.jquery.com/jquery.js"></script>
        <script>window.jQuery || document.write('<script src="{{ asset('bootstrap3/js/jquery.min.js') }}"><\/script>')</script>
        <script type="text/javascript" src="{{ asset('bootstrap3/js/bootstrap.min.js') }}"></script>
        {% block addjs %}{% endblock %}
    {% endblock %}
    </body>
</html>